<?xml version="1.0" encoding="utf-8"?>
<service>
<short>{{ item.name }}</short>
<description>
File generated by Ansible
</description>
{% if (item.ports.tcp is defined) and item.ports.tcp %}
{% for port in item.ports.tcp %}
<port protocol="tcp" port="{{ port }}" />
{% endfor %}
{% endif %}
{% if (item.ports.udp is defined) and item.ports.udp %}
{% for port in item.ports.udp %}
<port protocol="udp" port="{{ port }}" />
{% endfor %}
{% endif %}
</service>
